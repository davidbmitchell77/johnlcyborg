<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Enter_Account_First_and_Last_Name</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  OR(RecordTypeName__c = &quot;Patient&quot;, RecordTypeName__c = &quot;Provider&quot;),
  NOT(CONTAINS(Name, &quot; &quot;))
)</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>Please enter a Vails first and last name</errorMessage>
</ValidationRule>
